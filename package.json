{
    "name": "ianarber",
    "version": "0.0.0",
    "description": "Bio wesbite for composer Ian Arber. Built on Jekyll using Gulp",
    "main": "gulpfile.js",
    "author": "Anthony Desmier",
    "devDependencies": {
        "gulp": "^3.8.8",
        "gulp-sass": "^2.1.0",
        "browser-sync": "^1.3.7",
        "gulp-autoprefixer": "1.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://ianarber@bitbucket.org/ianarber/ianarber.git"
    },
    "_aerobatic": {
        "router": [
            {
                "module": "basic-auth",
                "path": "/clientarea",
                "options": {
                    "username": "$BASIC_AUTH_USERNAME",
                    "password": "$BASIC_AUTH_PASSWORD"
                }
          },
            {
                "module": "custom-errors",
                "options": {
                    "errors": {
                        "404": "/404.html"
                    }
                }
          },
            {
                "module": "webpage"
          }
        ],
        "build": {
            "engine": "jekyll"
        }
    }
}