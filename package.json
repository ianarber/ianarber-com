{
    "name": "site-ianarber",
    "version": "0.0.0",
    "description": "Bio wesbite for composer Ian Arber. Built on Jekyll using Gulp",
    "main": "gulpfile.js",
    "author": "Anthony Desmier",
    "devDependencies": {
        "gulp": "^3.8.8",
        "gulp-sass": "^2.1.0",
        "browser-sync": "^1.3.7",
        "gulp-autoprefixer": "1.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://adesmier@bitbucket.org/adesmier/site-ianarber.git"
    }, 
    "_aerobatic": {
        "omitClientConfigVar": true,
        "router": [
          {
            "module": "basic-auth",
            "path": "/clientarea",
            "options": {
              "username": "$BASIC_AUTH_USERNAME",
              "password": "$BASIC_AUTH_PASSWORD"
            }
          },
          {
            "module": "webpage"
          }
        ],
        "build": {
        "engine": "jekyll"
    } 
  }  
}